@startuml
skinparam linetype ortho

title methodA(exam);

rectangle "Stack" {
	rectangle "main" {
		rectangle "exam"
	}
}

cloud "Heap" {
	rectangle "Exam:1000" as Exam1000 {
		rectangle "num = 2" as num1000
		rectangle "str = "str1"" as str1000
		num1000 -[hidden]d- str1000
	}
}

Stack -[hidden]right-Heap

exam -right-> Exam1000

@enduml